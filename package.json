		"coverage-quiet": "covert test.js --quiet",
		"lint": "jscs *.js"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/is-nan.git"
	},
	"bugs": {
		"url": "https://github.com/ljharb/is-nan/issues"
	},
	"homepage": "https://github.com/ljharb/is-nan",
	"keywords": [
		"is",
		"NaN",
		"not a number",
		"number",
		"isNaN",
		"ES6",
		"shim",
		"polyfill"
	],
	"dependencies": {
		"jscs": "~1.5.8"
	},
	"devDependencies": {
		"tape": "~2.13.1",
		"covert": "~0.4.0"
	},
